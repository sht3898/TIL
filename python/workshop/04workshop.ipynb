   "source": [
    "def my_sqrt(n):\n",
    "    a, b = 1, n\n",
    "    result = 1\n",
    "    while abs(result ** 2 - n) >= 1e-10:\n",
    "        result = (a+b)/2\n",
    "        if result ** 2 < n:\n",
    "            a = result\n",
    "        else:\n",
    "            b = result\n",
    "    return(result)"
   ]
  },
